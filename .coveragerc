[run]
source = src
omit =
    */tests/*
    */__pycache__/*
    */venv/*
    */env/*
    */.venv/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError
    except ImportError
    def run_all_stages 